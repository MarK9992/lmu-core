<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="lmu"
         name="lmu"
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               id="lmugeneration"
               label="Lmu">
            <command
                  commandId="fr.unice.polytech.retro.lmu-plugin.generation"
                  label="generation"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="fr.unice.polytech.retro.handler.GenerateHandler"
            id="fr.unice.polytech.retro.lmu-plugin.generation"
            name="generate">
      </command>
   </extension>
   <extension
         id="generationHandler"
         point="org.eclipse.ui.handlers">
      <handler
            commandId="fr.unice.polytech.retro.lmu-plugin.handlergenerate">
      </handler>
   </extension>


</plugin>
